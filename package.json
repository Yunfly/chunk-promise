{
  "name": "@yunfly/chunk-promise",
  "version": "1.0.5",
  "description": "This module breaks the Promise into chunks and executes it in turn",
  "scripts": {
    "test": "mocha -r ts-node/register ./test/*.ts",
    "cover": "nyc mocha test/*.ts",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yunfly/chunk-promise.git"
  },
  "nyc": {
    "include": [
      "lib/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true
  },
  "keywords": [
    "loop",
    "chunks",
    "promise",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "unpkg": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Yunfly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yunfly/chunk-promise/issues"
  },
  "homepage": "https://github.com/Yunfly/chunk-promise#readme",
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}